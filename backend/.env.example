# TunjiaX Backend Environment Variables
# Copy this file to .env and fill in the values

# === ElevenLabs Integration ===
ELEVENLABS_CUSTOM_LLM_SECRET=your_elevenlabs_custom_llm_secret

# === Google Cloud Platform ===
GOOGLE_APPLICATION_CREDENTIALS=./tunjiax-wallet-key.json
GCP_PROJECT=tunjiax-wallet-482614
GCP_LOCATION=us-central1

# === Cloud SQL Database ===
CLOUD_SQL_CONNECTION_NAME=tunjiax-wallet-482614:us-central1:tunjiax-db
DB_PASSWORD=your_database_password

# === Authentication ===
JWT_SECRET_KEY=your_jwt_secret_key

# Google OAuth Client ID (same value used in frontend as VITE_GOOGLE_CLIENT_ID)
GOOGLE_CLIENT_ID=your_google_oauth_client_id
